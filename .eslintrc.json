{
	"extends": "airbnb/base",
	"env": {
		"node": true
	},
	"rules": {
		"indent": [2, "tab", {"SwitchCase": 1}],
		"comma-dangle": [2, "always-multiline"],
		"keyword-spacing": [2, {
			"before": true,
			"after": false,
			"overrides": {
				"else": {"after": true},
				"return": {"after": true},
				"case": {"after": true},
				"let": {"after": true},
				"const": {"after": true},
				"from": {"after": true},
				"import": {"after": true}
			}
		}],
		"max-len": [1, 150, 2],
		"object-curly-spacing": [2, "never"],
		"space-before-function-paren": [2, "never"],
		"strict": [2, "global"],
		"no-else-return": 0,
		"no-param-reassign": 0,
		"no-console": 0,
		"no-unused-vars": [2, {
			"vars": "all",
			"args": "none"
		}],
		"consistent-return": 0,
		"object-shorthand": 1,
		"prefer-rest-params": 0,
		"no-confusing-arrow": [2, {"allowParens": true}]
	}
}
